<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.walker.dao.SimpleDao" >
  
  <select id="getSimple" resultType="com.walker.bean.Simple">
  		select * from simple
  </select>
  
  <insert id="insertSimple">
  		insert into simple(name,age)values(#{name},#{age})
  </insert>
</mapper>